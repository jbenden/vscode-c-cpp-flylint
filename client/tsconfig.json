{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es6"
        ],
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "out",
        "newLine": "LF",
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "exclude": [
        "node_modules",
        "server"
    ]
}
